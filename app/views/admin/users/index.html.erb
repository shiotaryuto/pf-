<div class="container">
    <li class="navbar-brand p-3">
     <h1>会員一覧</h1>
    </li>
<table border="1", style= "border-collapse: collapse">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>ニックネーム</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>

  <% @users.each do |user| %>
  <tbody>
  <tr>
    <td> <%= user.id %></td>
    <td><%= link_to(admin_user_path(user.id)) do %>
    <%= user.first_name %><%= user.last_name %>
    <% end %>
    </td>
    <td><%= user.nickname %></td>
    <td><%= user.email %></td>
    <td><%= user.is_active %></td>
  </tr>
  <% end %>
</tbody>
</table>
</div>